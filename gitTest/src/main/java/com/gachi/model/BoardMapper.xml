<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gachi.model.BoardMapper">

<!-- 게시판 전체 내용 조회할 수 있는 SQL문장 -->
<select id="AllList" resultType="com.gachi.model.BoardDTO">
	select * from tbl_post
</select>

<!-- 게시글 하나만 조회하는 sql문장 -->
<select id="detail" parameterType="string" resultType="com.gachi.model.BoardDTO">
	select * from web_board where num=#{num}
</select>



</mapper>